{
  "version": 2,
  "name": "vn-tour-travels-frontend",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api|_next|_static|favicon.ico|robots.txt|sitemap.xml|manifest.json|assets).*)",
      "destination": "/index.html"
    }
  ]
}
